<?xml version="1.0" encoding="UTF-8" ?>
<injection>
  <strategies>
    <strategy>
      <type>java.util.concurrent.ThreadFactory</type>

      <staticMethod>
        java.util.concurrent.Executors#defaultThreadFactory
      </staticMethod>
    </strategy>

    <strategy>
      <type>java.util.concurrent.ExecutorService</type>

      <staticMethod>
        java.util.concurrent.Executors#newSingleThreadPoolExecutor
      </staticMethod>

      <dependencies>
        <dependency>
          <type>java.util.concurrent.ThreadFactory</type>
        </dependency>
      </dependencies>
    </strategy>

  </strategies>
</injection>